{
  "name": "unie",
  "version": "0.1.0",
  "author": "devMentorship",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "concurrently \"next dev\" \"cypress open\"",
    "test:cypress": "start-server-and-test start http://localhost:3000 cypress:interface",
    "cypress:interface": "cypress open",
    "cypress:terminal": "cypress run",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc",
    "eslint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "eslint-fix": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --fix",
    "stylelint": "stylelint **/*.css",
    "stylelint-fix": "stylelint **/*.css --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@portabletext/react": "^2.0.2",
    "antd": "^5.4.6",
    "axios": "^1.4.0",
    "concurrently": "^8.0.1",
    "next": "13.2.4",
    "next-sanity-image": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.34.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "cypress": "^12.12.0",
    "cypress-terminal-report": "^5.1.1",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.0",
    "prettier": "2.6.2",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^14.2.0",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-order": "^2.0.0",
    "typescript": "5.0.3"
  }
}
